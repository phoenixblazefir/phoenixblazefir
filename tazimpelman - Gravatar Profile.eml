footer { background: rgb(255, 255, 255); }

.g-footer { font-size: 16px; }

.g-footer .g-footer__external-link::after { content: "â†—"; display: inline-block; text-decoration: none; white-space: nowrap; transition: transform 0.15s ease-out 0s; pointer-events: none; position: relative; top: 0.055em; left: 0.165em; margin-right: -0.095em; font-weight: 700; transform: scale3d(0.55, 0.55, 1); transform-origin: 0px 0px; }

.g-footer .g-footer__external-link:hover::after { transform: scale3d(0.55, 0.55, 1) translate3d(0.065em, -0.065em, 0px); }

.g-footer .g-footer__grid { display: grid; grid-template-columns: repeat(auto-fit, minmax(340px, 1fr)); column-gap: ; row-gap: 48px; }

.g-footer .g-footer__top { background: rgb(29, 79, 196); }

.g-footer .g-footer__top .g-footer__top-content { padding: 48px 0px; display: grid; grid-template-columns: repeat(auto-fit, minmax(340px, 1fr)); column-gap: ; margin: 0px auto; max-width: 1056px; }

.g-footer .g-footer__top .g-footer__top-content a { color: rgb(255, 255, 255); }

